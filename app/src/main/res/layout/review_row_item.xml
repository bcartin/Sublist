<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
                                             xmlns:app="http://schemas.android.com/apk/res-auto"
                                             xmlns:tools="http://schemas.android.com/tools"
                                             android:layout_width="match_parent"
                                             android:layout_height="wrap_content">

    <ImageView
            android:layout_width="20dp"
            android:layout_height="20dp" app:srcCompat="@drawable/empty_star" android:id="@+id/star11"
            android:layout_weight="1" app:layout_constraintTop_toTopOf="parent"
            android:layout_marginTop="8dp" app:layout_constraintStart_toEndOf="@+id/reviewrTextView"
            android:layout_marginStart="8dp"/>
    <ImageView
            android:layout_width="24dp"
            android:layout_height="24dp" app:srcCompat="@drawable/aim" android:id="@+id/imageView11"
            android:layout_weight="1" android:layout_marginTop="8dp"
            app:layout_constraintTop_toTopOf="parent" app:layout_constraintStart_toEndOf="@+id/star15"
            android:layout_marginStart="24dp"/>
    <ImageView
            android:layout_width="20dp"
            android:layout_height="20dp" app:srcCompat="@drawable/empty_star" android:id="@+id/star12"
            android:layout_weight="1"
            app:layout_constraintTop_toTopOf="@+id/star11"
            app:layout_constraintStart_toEndOf="@+id/star11" android:layout_marginStart="2dp"/>
    <TextView
            android:text="Anonymous"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" android:id="@+id/reviewrTextView" android:layout_weight="1"
            android:textStyle="bold" android:textSize="16sp" android:textColor="@android:color/black"
            android:layout_marginTop="8dp"
            app:layout_constraintTop_toTopOf="parent" app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="8dp"/>
    <ImageView
            android:layout_width="20dp"
            android:layout_height="20dp" app:srcCompat="@drawable/empty_star" android:id="@+id/star14"
            android:layout_weight="1"
            app:layout_constraintTop_toTopOf="@+id/star13" android:layout_marginStart="2dp"
            app:layout_constraintStart_toEndOf="@+id/star13"/>
    <ImageView
            android:layout_width="20dp"
            android:layout_height="20dp" app:srcCompat="@drawable/empty_star" android:id="@+id/star15"
            android:layout_weight="1"
            app:layout_constraintTop_toTopOf="@+id/star14" android:layout_marginStart="2dp"
            app:layout_constraintStart_toEndOf="@+id/star14"/>
    <ImageView
            android:layout_width="20dp"
            android:layout_height="20dp" app:srcCompat="@drawable/empty_star" android:id="@+id/star13"
            android:layout_weight="1"
            app:layout_constraintTop_toTopOf="@+id/star12" android:layout_marginStart="2dp"
            app:layout_constraintStart_toEndOf="@+id/star12"/>
    <LinearLayout
            android:orientation="horizontal"
            android:layout_width="0dp"
            android:layout_height="wrap_content" android:layout_marginTop="8dp"
            app:layout_constraintTop_toBottomOf="@+id/star15" app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="8dp" app:layout_constraintEnd_toEndOf="parent" android:layout_marginEnd="8dp"
            android:id="@+id/linearLayout4">
        <android.support.constraint.ConstraintLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_weight="1">

            <ImageView
                    android:layout_width="20dp"
                    android:layout_height="20dp" app:srcCompat="@drawable/empty_star" android:id="@+id/star21"
                    android:layout_weight="1"
                    app:layout_constraintStart_toStartOf="parent" app:layout_constraintTop_toTopOf="parent"
                    android:layout_marginTop="8dp"/>
            <ImageView
                    android:layout_width="20dp"
                    android:layout_height="20dp" app:srcCompat="@drawable/empty_star" android:id="@+id/star22"
                    android:layout_weight="1"
                    app:layout_constraintTop_toTopOf="@+id/star21"
                    app:layout_constraintStart_toEndOf="@+id/star21"/>
            <ImageView
                    android:layout_width="20dp"
                    android:layout_height="20dp" app:srcCompat="@drawable/empty_star" android:id="@+id/star23"
                    android:layout_weight="1"
                    app:layout_constraintTop_toTopOf="@+id/star22"
                    app:layout_constraintStart_toEndOf="@+id/star22"/>
            <ImageView
                    android:layout_width="20dp"
                    android:layout_height="20dp" app:srcCompat="@drawable/empty_star" android:id="@+id/star24"
                    android:layout_weight="1"
                    app:layout_constraintTop_toTopOf="@+id/star23"
                    app:layout_constraintStart_toEndOf="@+id/star23"/>
            <ImageView
                    android:layout_width="20dp"
                    android:layout_height="20dp" app:srcCompat="@drawable/empty_star" android:id="@+id/star25"
                    android:layout_weight="1"
                    app:layout_constraintTop_toTopOf="@+id/star24"
                    app:layout_constraintStart_toEndOf="@+id/star24"/>
            <TextView
                    android:text="Workmanship"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:id="@+id/workmanshipTextView" android:textStyle="italic"
                    android:textColor="@android:color/black"
                    app:layout_constraintStart_toEndOf="@+id/star25" android:layout_marginStart="4dp"
                    android:textSize="12sp"
                    android:layout_marginTop="8dp" app:layout_constraintTop_toTopOf="parent"/>
        </android.support.constraint.ConstraintLayout>
        <android.support.constraint.ConstraintLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_weight="1">

            <ImageView
                    android:layout_width="20dp"
                    android:layout_height="20dp" app:srcCompat="@drawable/empty_star" android:id="@+id/star31"
                    android:layout_weight="1"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="parent"
                    android:layout_marginTop="8dp"/>
            <ImageView
                    android:layout_width="20dp"
                    android:layout_height="20dp" app:srcCompat="@drawable/empty_star" android:id="@+id/star35"
                    android:layout_weight="1"
                    app:layout_constraintTop_toTopOf="@+id/star34"
                    app:layout_constraintStart_toEndOf="@+id/star34"/>
            <ImageView
                    android:layout_width="20dp"
                    android:layout_height="20dp" app:srcCompat="@drawable/empty_star" android:id="@+id/star32"
                    android:layout_weight="1"
                    app:layout_constraintTop_toTopOf="@+id/star31"
                    app:layout_constraintStart_toEndOf="@+id/star31"/>
            <ImageView
                    android:layout_width="20dp"
                    android:layout_height="20dp" app:srcCompat="@drawable/empty_star" android:id="@+id/star33"
                    android:layout_weight="1"
                    app:layout_constraintTop_toTopOf="@+id/star32"
                    app:layout_constraintStart_toEndOf="@+id/star32"/>
            <ImageView
                    android:layout_width="20dp"
                    android:layout_height="20dp" app:srcCompat="@drawable/empty_star" android:id="@+id/star34"
                    android:layout_weight="1"
                    app:layout_constraintTop_toTopOf="@+id/star33"
                    app:layout_constraintStart_toEndOf="@+id/star33"/>
            <TextView
                    android:text="On Budget"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:id="@+id/onBudgetTextView" android:textStyle="italic"
                    android:textColor="@android:color/black"
                    android:layout_marginTop="8dp"
                    app:layout_constraintTop_toTopOf="parent"
                    app:layout_constraintStart_toEndOf="@+id/star35" android:layout_marginStart="4dp"
                    android:textSize="12sp"/>
        </android.support.constraint.ConstraintLayout>
    </LinearLayout>
    <LinearLayout
            android:orientation="horizontal"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="8dp" app:layout_constraintEnd_toEndOf="parent"
            android:layout_marginEnd="8dp" android:layout_marginTop="8dp"
            app:layout_constraintTop_toBottomOf="@+id/linearLayout4" android:id="@+id/linearLayout5">
        <android.support.constraint.ConstraintLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_weight="1">

            <ImageView
                    android:layout_width="20dp"
                    android:layout_height="20dp" app:srcCompat="@drawable/empty_star" android:id="@+id/star41"
                    android:layout_weight="1"
                    app:layout_constraintTop_toTopOf="parent" android:layout_marginTop="8dp"
                    app:layout_constraintStart_toStartOf="parent"/>
            <ImageView
                    android:layout_width="20dp"
                    android:layout_height="20dp" app:srcCompat="@drawable/empty_star" android:id="@+id/star42"
                    android:layout_weight="1"
                    app:layout_constraintTop_toTopOf="@+id/star41"
                    app:layout_constraintStart_toEndOf="@+id/star41"/>
            <ImageView
                    android:layout_width="20dp"
                    android:layout_height="20dp" app:srcCompat="@drawable/empty_star" android:id="@+id/star43"
                    android:layout_weight="1"
                    app:layout_constraintTop_toTopOf="@+id/star42"
                    app:layout_constraintStart_toEndOf="@+id/star42"/>
            <ImageView
                    android:layout_width="20dp"
                    android:layout_height="20dp" app:srcCompat="@drawable/empty_star" android:id="@+id/star44"
                    android:layout_weight="1"
                    app:layout_constraintTop_toTopOf="@+id/star43"
                    app:layout_constraintStart_toEndOf="@+id/star43"/>
            <ImageView
                    android:layout_width="20dp"
                    android:layout_height="20dp" app:srcCompat="@drawable/empty_star" android:id="@+id/star45"
                    android:layout_weight="1"
                    app:layout_constraintTop_toTopOf="@+id/star44"
                    app:layout_constraintStart_toEndOf="@+id/star44"/>
            <TextView
                    android:text="On Schedule"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:id="@+id/onScheduleTextView" android:textStyle="italic"
                    android:textColor="@android:color/black"
                    app:layout_constraintStart_toEndOf="@+id/star45" android:layout_marginStart="4dp"
                    android:textSize="12sp"
                    android:layout_marginTop="8dp" app:layout_constraintTop_toTopOf="parent"/>
        </android.support.constraint.ConstraintLayout>
        <android.support.constraint.ConstraintLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_weight="1">

            <ImageView
                    android:layout_width="20dp"
                    android:layout_height="20dp" app:srcCompat="@drawable/empty_star" android:id="@+id/star51"
                    android:layout_weight="1"
                    app:layout_constraintTop_toTopOf="parent" android:layout_marginTop="8dp"
                    app:layout_constraintStart_toStartOf="parent"/>
            <ImageView
                    android:layout_width="20dp"
                    android:layout_height="20dp" app:srcCompat="@drawable/empty_star" android:id="@+id/star55"
                    android:layout_weight="1"
                    app:layout_constraintTop_toTopOf="@+id/star54"
                    app:layout_constraintStart_toEndOf="@+id/star54"/>
            <ImageView
                    android:layout_width="20dp"
                    android:layout_height="20dp" app:srcCompat="@drawable/empty_star" android:id="@+id/star52"
                    android:layout_weight="1"
                    app:layout_constraintTop_toTopOf="@+id/star51"
                    app:layout_constraintStart_toEndOf="@+id/star51"/>
            <ImageView
                    android:layout_width="20dp"
                    android:layout_height="20dp" app:srcCompat="@drawable/empty_star" android:id="@+id/star53"
                    android:layout_weight="1"
                    app:layout_constraintTop_toTopOf="@+id/star52"
                    app:layout_constraintStart_toEndOf="@+id/star52"/>
            <ImageView
                    android:layout_width="20dp"
                    android:layout_height="20dp" app:srcCompat="@drawable/empty_star" android:id="@+id/star54"
                    android:layout_weight="1"
                    app:layout_constraintTop_toTopOf="@+id/star53"
                    app:layout_constraintStart_toEndOf="@+id/star53"/>
            <TextView
                    android:text="Communication"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:id="@+id/communicationTextView" android:textStyle="italic"
                    android:textColor="@android:color/black"
                    android:layout_marginTop="8dp"
                    app:layout_constraintTop_toTopOf="parent"
                    app:layout_constraintStart_toEndOf="@+id/star55" android:layout_marginStart="4dp"
                    android:textSize="12sp"/>
        </android.support.constraint.ConstraintLayout>
    </LinearLayout>
    <TextView
            android:text="01/15/2019"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/dateTextView" android:textStyle="italic" app:layout_constraintEnd_toEndOf="parent"
            android:layout_marginEnd="8dp" android:layout_marginTop="8dp" app:layout_constraintTop_toTopOf="parent"/>
    <TextView
            android:text="Metal"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:id="@+id/featureTextView" android:textStyle="italic"
            app:layout_constraintStart_toEndOf="@+id/imageView11" android:layout_marginStart="4dp"
            app:layout_constraintTop_toTopOf="@+id/imageView11" android:layout_marginTop="2dp"
            android:layout_marginEnd="4dp" app:layout_constraintEnd_toStartOf="@+id/dateTextView"/>
    <TextView
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:id="@+id/commentTextView" android:layout_marginTop="8dp"
            app:layout_constraintTop_toBottomOf="@+id/linearLayout5" app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="24dp" app:layout_constraintEnd_toEndOf="parent"
            android:layout_marginEnd="24dp" android:textSize="12sp" android:textColor="@android:color/black"
            android:textStyle="italic"/>
</android.support.constraint.ConstraintLayout>